{
  "name": "quizmaster-frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "vite --port 5175 --host 0.0.0.0",
    "dev:frontend": "vite --port 5175 --host 0.0.0.0",
    "dev:backend": "cd ../backend && python3 run.py",
    "dev:full": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\" --names \"Backend,Frontend\" --prefix-colors \"blue,green\"",
    "build": "vite build",
    "preview": "vite preview",
    "start": "npm run dev:full"
  },
  "dependencies": {
    "vue": "^3.3.8",
    "vue-router": "^4.2.5",
    "vuex": "^4.1.0",
    "axios": "^1.6.2",
    "bootstrap": "^5.3.2",
    "bootstrap-icons": "^1.11.2",
    "@popperjs/core": "^2.11.8",
    "chart.js": "^4.4.1",
    "vue-chartjs": "^5.3.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.5.0",
    "vite": "^5.0.0",
    "concurrently": "^8.2.2"
  }
} 